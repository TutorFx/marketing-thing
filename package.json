{
  "name": "nuxt-app",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "bunx prisma generate && bun --bun nuxt build",
    "dev": "bun --bun nuxt dev",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "seed": "bun prisma/seed.ts",
    "prisma:generate": "bunx prisma generate --no-engine",
    "migrate:dev": "bunx prisma migrate dev --preview-feature",
    "migrate:dev:create": "bunx prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "bunx prisma migrate reset --preview-feature",
    "migrate:deploy": "bunx prisma migrate deploy --preview-feature",
    "migrate:status": "bunx prisma migrate status --preview-feature",
    "migrate:resolve": "bunx prisma migrate resolve --preview-feature",
    "prisma:studio": "bunx prisma studio",
    "check": "bun run build && node --env-file=.env .output/server/index.mjs",
    "release": "changelogen --release"
  },
  "dependencies": {
    "@nuxt/content": "^3.0.0",
    "@nuxt/ui": "^3.0.0-alpha.10",
    "@prisma/client": "^6.1.0",
    "changelogen": "^0.5.7",
    "nuxt": "^3.14.1592",
    "nuxt-auth-utils": "^0.5.7",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@google/generative-ai": "^0.21.0",
    "@nuxt/eslint": "^0.7.4",
    "@nuxtjs/mdc": "^0.11.1",
    "@pinia/nuxt": "^0.9.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tiptap/extension-placeholder": "^2.10.4",
    "@tiptap/extension-underline": "^2.10.4",
    "@tiptap/starter-kit": "^2.10.4",
    "@tiptap/vue-3": "^2.10.4",
    "@types/bun": "^1.1.14",
    "@vueuse/core": "^12.2.0",
    "bun": "^1.1.42",
    "date-fns": "^4.1.0",
    "diff-match-patch-es": "^0.1.1",
    "eslint-plugin-format": "^0.1.3",
    "fast-xml-parser": "^4.5.1",
    "json5": "^2.2.3",
    "pinia": "^2.3.0",
    "safe-eval": "^0.4.1",
    "tailwind-variants": "^0.3.0",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  }
}
